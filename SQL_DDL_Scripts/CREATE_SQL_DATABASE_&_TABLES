SELECT * FROM cst339.orders;CREATE DATABASE `dememerr` /*!40100 DEFAULT CHARACTER SET utf8 */;
CREATE TABLE `employee` (
  `EmployeeId` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `UserName` varchar(200) NOT NULL,
  `Password` varchar(200) NOT NULL,
  `FirstName` varchar(200) NOT NULL,
  `LastName` varchar(200) NOT NULL,
  `Email` varchar(200) NOT NULL,
  PRIMARY KEY (`EmployeeId`),
  UNIQUE KEY `EmployeeId_UNIQUE` (`EmployeeId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `users` (
  `UserId` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `UserName` varchar(200) NOT NULL,
  `FirstName` varchar(200) NOT NULL,
  `LastName` varchar(200) NOT NULL,
  `Email` varchar(200) NOT NULL,
  `Address` varchar(200) DEFAULT NULL,
  `Phone` varchar(15) DEFAULT NULL,
  PRIMARY KEY (`UserId`),
  UNIQUE KEY `UserId_UNIQUE` (`UserId`),
  UNIQUE KEY `Username_UNIQUE` (`UserName`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `friendslist` (
  `FriendsListId` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `UserId` int(11) NOT NULL,
  `FriendId` int(11) NOT NULL,
  PRIMARY KEY (`FriendsListId`),
  UNIQUE KEY `FriendsListId_UNIQUE` (`FriendsListId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `postresponses` (
  `PostResponseId` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `ParentPostId` int(10) unsigned NOT NULL,
  `ChildPostId` int(10) unsigned NOT NULL,
  PRIMARY KEY (`PostResponseId`),
  UNIQUE KEY `PostResponseId_UNIQUE` (`PostResponseId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `posts` (
  `PostId` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `UserId` int(11) NOT NULL,
  `ImageLocation` varchar(8000) NOT NULL,
  `NumberOfLikes` int(10) unsigned NOT NULL,
  `NumberOfDislikes` int(10) unsigned NOT NULL,
  `PostedOn` datetime NOT NULL,
  PRIMARY KEY (`PostId`),
  UNIQUE KEY `PostId_UNIQUE` (`PostId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
